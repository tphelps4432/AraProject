<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.tomphelps.araproject">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@android:style/Theme.NoTitleBar.Fullscreen">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".results.resultsMain"
            android:label="@string/title_activity_p_hmain"
            android:parentActivityName=".MainActivity"
            android:theme="@style/Base.Theme.AppCompat">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.tomphelps.araproject.MainActivity" />
        </activity>
        <activity
            android:name=".TestingActivity"
            android:label="@string/title_activity_testing"></activity>
    </application>

</manifest>
